<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>tth_Trip_Start_Date__c</fullName>
    <description>If using relative dates, Start date it today minus Days Ago or else it is just the absolute start Date</description>
    <externalId>false</externalId>
    <formula>IF( $CustomMetadata.tth_Booking_ReservationSettings__mdt.default.Use_Relative_Dates__c ,  TODAY() - tth_Days_Ago__c  ,  tth_Trip_Start__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Trip Start</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
