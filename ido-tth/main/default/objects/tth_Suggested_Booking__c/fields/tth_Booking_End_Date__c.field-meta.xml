<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>tth_Booking_End_Date__c</fullName>
    <description>If using relative dates, Today minus Days Ago Plus Number of Days, or else it is the End Date absolute value</description>
    <externalId>false</externalId>
    <formula>IF( $CustomMetadata.tth_Booking_ReservationSettings__mdt.default.Use_Relative_Dates__c ,(Today()- tth_Days_Ago__c) +  Number_of_Days__c  ,  tth_Booking_End__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Booking End</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
